{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "karma": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["webpack.*.js"]}],
        "no-underscore-dangle": "off"
    }
}
