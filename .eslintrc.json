{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": ["webpack.*.js", "sharp.js"]
            }
        ],
        "no-underscore-dangle": "off"
    }
}
